require(xtable)
####SPARSE###
####n=200 p=250####
t1.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset01")
t2.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset02")
t3.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset03")
t4.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset04")
t5.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset05")
t6.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset06")
t7.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset07")
t8.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset08")
t9.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset09")
t10.250S<-read.csv("/Users/alejandraavalospacheco/Dropbox/OxWaSP/Programing/BFA/Examples/Paper1806/Table1809SparseMn100p250q10BEDataset10")

#Table Paper
table1.250S=data.frame(q_hat=round(apply(cbind(t1.250S[,2],t2.250S[,2],t3.250S[,2],t4.250S[,2],t5.250S[,2],t6.250S[,2],t7.250S[,2],t8.250S[,2],t9.250S[,2],t10.250S[,2]),1,mean),1),
                       M_hat=round(apply(cbind(t1.250S[,3],t2.250S[,3],t3.250S[,3],t4.250S[,3],t5.250S[,3],t6.250S[,3],t7.250S[,3],t8.250S[,3],t9.250S[,3],t10.250S[,3]),1,mean),1),
                       FN_X=round(apply(cbind(t1.250S[,5],t2.250S[,5],t3.250S[,5],t4.250S[,5],t5.250S[,5],t6.250S[,5],t7.250S[,5],t8.250S[,5],t9.250S[,5],t10.250S[,5]),1,mean),1),
                       FN_ZM=round(apply(cbind(t1.250S[,4],t2.250S[,4],t3.250S[,4],t4.250S[,4],t5.250S[,4],t6.250S[,4],t7.250S[,4],t8.250S[,4],t9.250S[,4],t10.250S[,4]),1,mean),1),
                       FN_covX=round(apply(cbind(t1.250S[,6],t2.250S[,6],t3.250S[,6],t4.250S[,6],t5.250S[,6],t6.250S[,6],t7.250S[,6],t8.250S[,6],t9.250S[,6],t10.250S[,6]),1,mean),1),
                       it=round(apply(cbind(t1.250S[,7],t2.250S[,7],t3.250S[,7],t4.250S[,7],t5.250S[,7],t6.250S[,7],t7.250S[,7],t8.250S[,7],t9.250S[,7],t10.250S[,7]),1,mean),1))

rownames(table1.250S)<-c("Flat&S","Normal-SS&S","MOM-SS&S","ComBat-MLE&S","FastBFA&S","LASSO-BIC&S",
                         "Flat&NS","Normal-SS&NS","MOM-SS&NS","ComBat-MLE&NS","FastBFA&NS","LASSO-BIC&NS")

